-- For use in conjunction with the SPOC User Guide
-- See section on source-level Debugging
-- Debugging example code 1

PROC jim()
  PROC fred([]INT array,VAL INT a,VAL INT b)
    SEQ
      SEQ i = 0 FOR SIZE array
        IF
          (i \ 2)=0
            array[i] := a
  :
  INT a :
  VAL b IS 4 :
  SEQ
    a := 2
    [5]INT A : 
    INT j :
    SEQ
      fred(A,a,b)
      j := 0
      WHILE TRUE
        SEQ
          A[j] := j
          j := j + 1
: 
