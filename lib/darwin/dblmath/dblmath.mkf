dblmath=dblmath
dblmath-LBB=
$(dblmath).$(LIB): $(dblmath).$(T) $(dblmath).$(A) $(dblmath-LBB) $(dblmath).$(HDR)
	$(CAT) $(dblmath).$(T) $(dblmath-LBB) > $(dblmath).$(LIB)

