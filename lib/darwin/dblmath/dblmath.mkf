$(dblmath).$(O): $(dblmath).$(C) $(C2OBJ-DEP) $(dblmath-HDR)
	$(C2OBJ) $(dblmath).$(C) -o $(dblmath).$(O)

dblmath-DEP=$(SPOC)/lib/darwin/Intrinsics.lib 
$(dblmath).$(C): $(dblmath).$(OCC) $(dblmath-DEP) $(OCC2C-DEP)
	$(OCC2MOD) $(dblmath).$(OCC) -o $(dblmath).$(C)

dblmath-LHD=
$(dblmath).$(HDR): $(dblmath).$(C) $(dblmath-HDR) $(dblmath-LHD) $(C2PRO-DEP) 
	$(C2PRO) $(dblmath).$(C) -o $(dblmath).$(X)
	$(INVOKE) ./$(dblmath).$(X) $(dblmath-LHD) $(dblmath).$(HDR)
	$(RM) $(dblmath).$(X)

$(dblmath).$(T): $(dblmath).$(C)




