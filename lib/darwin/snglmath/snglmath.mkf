# Any command-line definitions:

snglmath=snglmath
snglmath-LBB=
$(snglmath).$(LIB): $(snglmath).$(T) $(snglmath).$(A) $(snglmath-LBB) $(snglmath).$(HDR)
	$(CAT) $(snglmath).$(T) $(snglmath-LBB) > $(snglmath).$(LIB)

snglmath-LDD=
$(snglmath).$(A): $(snglmath).$(O) $(snglmath-LDD)
	$(AR) $(snglmath).$(A) $(snglmath).$(O) $(snglmath-LDD)
