snglmath-HDR=$(SPOC)/lib/darwin/Intrinsics.hdr 
$(snglmath).$(O): $(snglmath).$(C) $(C2OBJ-DEP) $(snglmath-HDR)
	$(C2OBJ) $(snglmath).$(C) -o $(snglmath).$(O)

snglmath-DEP=$(SPOC)/lib/darwin/Intrinsics.lib 
$(snglmath).$(C): $(snglmath).$(OCC) $(snglmath-DEP) $(OCC2C-DEP)
	$(OCC2MOD) $(snglmath).$(OCC) -o $(snglmath).$(C)

snglmath-LHD=
$(snglmath).$(HDR): $(snglmath).$(C) $(snglmath-HDR) $(snglmath-LHD) $(C2PRO-DEP) 
	$(C2PRO) $(snglmath).$(C) -o $(snglmath).$(X)
	$(INVOKE) ./$(snglmath).$(X) $(snglmath-LHD) $(snglmath).$(HDR)
	$(RM) $(snglmath).$(X)

$(snglmath).$(T): $(snglmath).$(C)




